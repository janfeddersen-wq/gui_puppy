{
  "name": "gui_puppy",
  "version": "0.0.6",
  "description": "Electron GUI for code_puppy AI agent",
  "main": "src/main/index.js",
  "scripts": {
    "dev": "concurrently \"npm run vite:dev\" \"npm run electron:dev\"",
    "vite:dev": "vite",
    "electron:dev": "wait-on http://localhost:5173 && electron . --dev",
    "build": "vite build",
    "start": "npm run build && electron .",
    "preview": "vite preview",
    "dist": "npm run build && electron-builder --linux AppImage",
    "dist:clean": "rm -rf dist/ && npm run dist"
  },
  "keywords": [
    "electron",
    "ai",
    "code-generation"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.6",
    "@mui/material": "^7.3.6",
    "@types/dagre": "^0.7.53",
    "dagre": "^0.8.5",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-markdown": "^10.1.0",
    "reactflow": "^11.11.4",
    "remark-gfm": "^4.0.1",
    "socket.io-client": "^4.8.1"
  },
  "devDependencies": {
    "@types/node": "^25.0.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "concurrently": "^9.2.1",
    "electron": "^39.2.7",
    "electron-builder": "^25.1.8",
    "typescript": "^5.9.3",
    "vite": "^7.2.7",
    "wait-on": "^9.0.3"
  }
}
